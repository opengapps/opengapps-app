        <!--TITLES-->
            <string name="appintro_introslide_title">Вітаем у афіцыйным дадатку OpenGApps.org</string>
            <string name="appintro_storageperm_title">Доступ да сховішча</string>
            <string name="appintro_root_title">Правы суперкарыстальніка</string>
